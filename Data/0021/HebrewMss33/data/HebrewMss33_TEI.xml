      <sourceDesc>
        <msDesc>
          <msIdentifier>
            <country>United Kingdom</country>
            <settlement>Manchester</settlement>
            <institution>University of Manchester</institution>
            <repository>University of Manchester Library</repository>
            <collection>Hebrew Manuscripts</collection>
