    <marc:datafield ind1=" " ind2=" " tag="500">
  <marc:subfield code="a">Layout: Written in 23-26 long lines with poetry in two staggered lines; ruled with a mist&#x323;arah.</marc:subfield>
</marc:datafield>
    <marc:datafield ind1=" " ind2=" " tag="500">
