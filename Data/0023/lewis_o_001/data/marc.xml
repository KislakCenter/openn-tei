            <marc:subfield code="b">paper ;</marc:subfield>
            <marc:subfield code="c">533 x 368 (229) mm, bound to 535 x 370 mm.</marc:subfield>
        </marc:datafield>
        <marc:datafield tag="500" ind1=" " ind2=" ">
