              <author>
                <persName type="authority">Karmī, Marʻī ibn Yūsuf, -1623 or 1624</persName>
                <persName type="vernacular">كرمي، مرعي بن يوسف،</persName>
              </author>
              <respStmt>
                <resp>former owner</resp>
