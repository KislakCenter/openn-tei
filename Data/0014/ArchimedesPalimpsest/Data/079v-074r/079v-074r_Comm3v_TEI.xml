						/><w><unclear>κατηγο</unclear>ρεῖσθαι</w><pc>,</pc> καὶ Ἀθηναῖος κατὰ Σωκράτους<pc>.</pc>
					<gap quantity="3"/>
					<lb n="15" facs="#zComm3v_79v-74r1_15"/><w><supplied reason="lost">ἴσ</supplied>ως</w>
