                <resp>former owner</resp>
                <persName type="authority">Smith, David Eugene, 1860-1944</persName>
              </respStmt>
            </msItem>
            <msItem n="1r">
              <locus>1r</locus>
              <title>[al-Qur'ān]: Selections</title>
            </msItem>
            <msItem n="1r">
              <locus>1r</locus>
              <title>[al-Qur'ān].</title>
