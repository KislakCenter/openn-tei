    <datafield tag="520" ind1=" " ind2=" ">
      <subfield code="a">Complete copy of the Qurʼān with illuminations and gilding.</subfield>
    </datafield>
    <datafield tag="546" ind1=" " ind2=" ">
      <subfield code="a">Arabic.</subfield>
    </datafield>
    <datafield tag="500" ind1=" " ind2=" ">
