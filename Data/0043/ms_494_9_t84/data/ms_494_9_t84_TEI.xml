            </decoDesc>
            <bindingDesc>
              <binding>
                <p>Marbled paper wrapper, sewn as pamphlet.</p>
              </binding>
            </bindingDesc>
          </physDesc>
          <history>
            <origin>
              <origDate notBefore="1775" notAfter="1825"/>
              <p>1775-1825</p>
              <origPlace>[Turkey]</origPlace>
